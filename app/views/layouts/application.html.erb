  @block_tabel=         <%if @mond.block_tabel %><%= @mond.block_tabel %> <%else %>unknown!<%end %>
  @block_buchtabel=     <%if @mond.block_buchtabel %><%= @mond.block_buchtabel %> <%else %>unknown!<%end %><p>
<%end %>
<%if nil %>
    @block_mond=          <%if @mond.block_mond %> <%= @mond.block_mond %> <% else %>unknown!<% end %>
    @block_personal=      <%if @mond.block_personal %><%= @mond.block_personal %> <%else %>unknown!<%end %>
    @block_real_personal= <%if @mond.block_real_personal %><%= @mond.block_real_personal %> <%else %>unknown!<%end %>
    @block_comment=       <%if @mond.block_comment %><%= @mond.block_comment %> <%else %>unknown!<%end %>
    @block_timetabel=     <%if @mond.block_timetabel %><%= @mond.block_timetabel %> <%else %>unknown!<%end %>
    @block_tabel=         <%if @mond.block_tabel %><%= @mond.block_tabel %> <%else %>unknown!<%end %>
    @block_buchtabel=     <%if @mond.block_buchtabel %><%= @mond.block_buchtabel %> <%else %>unknown!<%end %><p>
