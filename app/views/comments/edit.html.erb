      <div class="content_2_col_comment"> <%= render('form_edit_comment') %></div>
    <% else %>
      <h1><button class="button"><%= link_to 'Вас нет доступа!', welcome_index_path %></button></h1>
    <% end %>
  </div>
</div>
</body>
<footer></footer>
</html>